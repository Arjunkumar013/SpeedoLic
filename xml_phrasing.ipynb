        "import os\n",
        "\n",
        "BASE_DATASET = \"/content/drive/MyDrive/kaggle_datasets/indian_vehicle_dataset\"\n",
        "\n",
        "IMG_DIR = os.path.join(BASE_DATASET, \"all_images\")\n",
        "XML_DIR = os.path.join(BASE_DATASET, \"all_xml\")\n",
        "\n",
